.btn {
  display: block;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 200px;
  color: #fff;
  text-align: center;
  background: -webkit-linear-gradient(left, #6f9eeb, #4787f4);
  background: -o-linear-gradient(right, #6f9eeb, #4787f4);
  background: -moz-linear-gradient(right, #6f9eeb, #4787f4);
  background: linear-gradient(to right, #6f9eeb, #4787f4);
  -webkit-box-shadow: 2px 2px 8px #4787f4;
  box-shadow: 2px 2px 8px #4787f4;
}

.btn.btn-square {
  border-radius: 5px;
  background-color: #3e86ff;
  width: 91.33%;
  box-shadow: none;
  margin-top: 50.52rpx;
  height: 98.94rpx;
  line-height: 98.94rpx;
}
page,.pages {
  width: 100%;
  height: 100%;
  font-size: 14px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
view, navigator,scroll-view, h1, h2, h3, h4, h5, h6, p, button, input, a {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

textarea {
  width: auto;
}
image {
   /* display: block;  */
  width: 100%;
  height: auto;
}
.relative{
    position: relative;
}
.absolute{
    position: absolute;
}
.flex {
    display: box; /* OLD - Android 4.4- */
    display: -webkit-box; /* OLD - iOS 6-, Safari 3.1-6 */
    display: -moz-box; /* OLD - Firefox 19- (buggy but mostly works) */
    display: -ms-flexbox; /* TWEENER - IE 10 */
    display: -webkit-flex; /* NEW - Chrome */
    display: flex;
}

.flex.newline {
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}

.flex.column {
    /* 09版 */
    -webkit-box-orient: vertical;
    /* 12版 */
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
    flex-direction: column;
}

.flex.row {
    /* 09版 */
    -webkit-box-orient: horizontal;
    /* 12版 */
    -webkit-flex-direction: row;
    -moz-flex-direction: row;
    -ms-flex-direction: row;
    -o-flex-direction: row;
    flex-direction: row;
}

.flex.ha {
    -moz-box-pack: space-around;
    -webkit-box-pack: space-around;
    -ms-flex-pack: space-around;
    -webkit-justify-content: space-around;
    justify-content: space-around;
}

.flex.hb {
    -moz-box-pack: space-between;
    -webkit-box-pack: space-between;
    -ms-flex-pack: center;
    -webkit-justify-content: space-between;
    justify-content: space-between;
}

.flex.hc {
    -moz-box-pack: center;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
}
.flex.hs{
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
}
.flex.he {
    -moz-box-pack: end;
    -webkit-box-pack: flex-end;
    -ms-flex-pack: end;
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
}

.flex.vc {
    -moz-box-align: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

.flex .shrink0 {
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
}

/* loading */

.loading {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 25;
  text-align: center;
}

.loading .wrap {
  width: 100%;
  margin-top: -240rpx;
}

.loading image {
  width: 100%;
  display: block;
}

.loading .logo {
  width: 45.33%;
  margin: 0 auto;
}

.loading .line {
  width: 45.67%;
  margin: 1.1rem auto 1.2rem;
  background: #f1f6ff;
  position: relative;
  height: 1px;
  overflow: hidden;
  border-radius: 100px;
}

.loading .line image {
  height: 1px;
  position: absolute;
  left: 0;
  top: 0;
  -webkit-animation: loadingLine 2s linear infinite;
  animation: loadingLine 2s linear infinite;
}

.loading .slogan {
  color: #2979ff;
  font-size: 0.9rem;
}

@-webkit-keyframes loadingLine {
  from {
    left: -100%;
  }

  to {
    left: 100%;
  }
}

@keyframes loadingLine {
  from {
    left: -100%;
  }

  to {
    left: 100%;
  }
}

