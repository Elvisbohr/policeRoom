/* 占位符 */

.place-holder {
  color: #bfbfbf;
  font-family: "微软雅黑";
}

/* 列表 */

.caption {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
  height: 96rpx;
  line-height: 96rpx;
