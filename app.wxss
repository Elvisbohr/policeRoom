  position: relative;
}

.caption-line {
  width: 4px;
  height: 50rpx;
  background: #fe6850;
  border-radius: 100px;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -25rpx;
}

.search-wrap {
  border: 1px solid #84b2ff;
  border-radius: 200px;
  width: 47%;
  height: 60rpx;
  padding: 0 3%;
