/* pages/sspDetail/sspDetail.wxss */
page{
  background: #f6f6f6;
}
.head{
  width: 100%;
  height: 200rpx;
  background-color: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  padding-top:10rpx;
}
.myplace{
  width: 100%;
  height: 200rpx;
}
.title{
  width:90%;
  font-size:18px;
  font-weight:bold;
  display:block;
  margin:0 auto;
  height:44px;
  word-break: break-all;
}
.subtitle {
  width:90%;
  position: absolute;
  left: 5%;
  bottom: 20rpx;
  font-size: 12px;
  color: #a0a0a0;
  overflow: hidden;
}
.con{
  box-sizing: border-box;
  padding: 30rpx;
}
.article-img-wrap{
  margin-bottom: 20rpx;
}
.article-img{
  width: 100%;
}
.preVideo-wrap{
  text-align: center;
  margin: 15rpx 0;
}