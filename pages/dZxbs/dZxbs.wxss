/* pages/dZxbs/dZxbs.wxss */
.scroll{
  width:100%;
  height:100%;
}
.banner {
    width: 100%;
    height: 22%;
    position: relative;
    background-repeat: no-repeat;
    background-size: 100% auto;
    margin-bottom: 10rpx;
    background-image: url('https://jws.kulizhi.com:443/jws/upload/head/2017/8/201708181922496415.jpg');
}
/* 头条 */
.new {
    width: 100%;
    height: 140rpx;
    border-top: 20rpx solid #f3f3f3;
    background:#fff;
}
.top{
  position: fixed;
  top:0;
  left:0;
}
.newMain {
    height: 100rpx;
}
.newL {
    width: 194rpx;
    height: inherit;
    color: #fe6850;
    font-size: 32rpx;
    text-align: center;
    font-weight: bold;
    border-right: 1px solid #e5e5e5;
    padding: 10rpx 0;
}
.newR {
    width: 550rpx;
    color: #535353;
    height: inherit;
    font-size: 26rpx;
}

.newR swiper {
    height: inherit;
    width: inherit;
    padding: 10rpx 0 10rpx 16rpx;
}
.newItemList {
    width: 85%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    height: 40rpx;
}
/* 在线办事 */
.myplace{
  height: 60rpx;
}
.myline{
  border-bottom: 1rpx solid #e8e8e8;
  margin-bottom: 30rpx;
}
.catalog-wrap{
  display: flex;
  text-align: center;
  height: 60rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
  background-color: white;
}
.catalog-big{
  flex: 1 0 auto;
  border-bottom: 1rpx solid #e8e8e8;
  padding-bottom: 10rpx;
}
.catalog-big.active{
  color: #3e86ff;
  border-bottom: 1rpx solid #3e86ff;
}
.cata-small-wrap{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 30rpx 0;
}
.cata-small{
  flex: 0 0 25vw;
  text-align: center;
  padding: 0 20rpx;
  font-size: 26rpx;
}
.icon{
  height: 65rpx;
  margin-bottom: 10rpx;
}
.myiconfont{
  font-size: 76rpx;
  margin-bottom: 10rpx;
}
.second-level{
  margin: 10rpx 0 20rpx 0;
  padding: 10rpx 20rpx;
  font-size: 32rpx;
  height: 90rpx;
  border-top:20rpx solid #eee;
}
.second-level:first-child{
  border:0;
}